    "lucide-react": "^0.303.0",
    "next": "14.2.32",
    "postcss": "^8.4.33",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "tailwindcss": "^3.4.3",
    "use-sync-external-store": "^1.5.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.57.0",
