{
  "name": "gliter-app",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.7.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p ${PORT:-8080}",
    "lint": "next lint",
    "deploy:check": "firebase --version && node --version && npm --version",
    "deploy:prod": "npm run build && firebase deploy --only apphosting:web-prod --project soygay-b9bc5",
